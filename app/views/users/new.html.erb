<h1>Create an Account</h1>

<%= form_for @user do |f| %>
<%= f.label :username %>
<%= f.text_field :username %>
<%= f.label :password %>
<%= f.password_field :password %>
<%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation %> <br>
<%= f.submit "Create User" %> <br>

<% end %>
